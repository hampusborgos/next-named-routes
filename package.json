{
  "name": "next-named-routes",
  "version": "0.1.0",
  "description": "Put enum names on next routes, to make programatic handling of routes easier.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "tsc -d",
    "prepush": "npm run format:check && npm run test",
    "prepublishOnly": "npm run test && npm run build",
    "format": "prettier --write './tests/*.ts' index.ts",
    "format:check": "prettier -l './tests/*.ts' index.ts",
    "test": "rm -f tests/*.js index.js && jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hjnilsson/next-named-routes.git"
  },
  "keywords": [
    "nextjs",
    "next",
    "typescript",
    "router",
    "route"
  ],
  "author": "Hampus Borgos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hjnilsson/next-named-routes/issues"
  },
  "homepage": "https://github.com/hjnilsson/next-named-routes#readme",
  "devDependencies": {
    "@types/jest": "26.0.20",
    "jest": "26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  }
}
