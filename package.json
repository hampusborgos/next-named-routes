{
  "name": "next-named-routes",
  "version": "0.1.0",
  "description": "Put enum names on next routes, to make programatic handling of routes easier.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "tsc -d",
    "prepush": "npm run format:check && npm run test",
    "prepublishOnly": "npm run test && npm run build",
    "format": "prettier --write './**/*.js'",
    "format:check": "prettier -l './**/*.js'",
    "test": "rm -f format*/*.js && jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hjnilsson/next-named-routes.git"
  },
  "keywords": [
    "nextjs",
    "next",
    "typescript",
    "router",
    "route"
  ],
  "author": "Hampus Borgos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hjnilsson/next-named-routes/issues"
  },
  "homepage": "https://github.com/hjnilsson/next-named-routes#readme",
  "devDependencies": {
    "@types/jest": "24.0.19",
    "jest": "24.9.0",
    "ts-jest": "24.1.0",
    "typescript": "3.7.2"
  }
}
